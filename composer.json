    "Twilio"
  ],
  "homepage": "https://github.com/mblackford/BlackfordTwilioBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Matthew Blackford",
      "email": "matthew.blackford@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.3.2",
    "symfony/framework-bundle": ">=2.8",
    "twilio/sdk": "~5.0"
  },
  "require-dev": {

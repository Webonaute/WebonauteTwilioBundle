    "Twilio"
  ],
  "homepage": "https://github.com/Webonaute/WebonauteTwilioBundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Mathieu Delisle",
      "email": "mdelisle@webonaute.ca"
    },
    {
      "name": "Matthew Blackford",
      "email": "matthew.blackford@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
